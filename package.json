{
  "name": "sf-api",
  "version": "1.0.0",
  "description": "sf api custom",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon",
    "lint-gql": "graphql-schema-linter {src,packages}/**/*.graphqls",
